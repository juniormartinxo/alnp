{
  "name": "calnp",
  "version": "1.0.0",
  "description": "Repositório para armazenar o código gerado dirante ao Módulo 4 do Curso de Arquitetura Limpa na Prática do Professor Otávio Lemos",
  "main": "main.ts",
  "scripts": {
    "test": "jest",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juniormartinxo/calnp.git"
  },
  "keywords": [
    "clean-code",
    "clean-architecture",
    "arquitectura-de-software",
    "arquitetura-limpa"
  ],
  "author": "Junior Martins <amjr,box@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/juniormartinxo/calnp/issues"
  },
  "homepage": "https://github.com/juniormartinxo/calnp#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@jest/globals": "^29.4.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.19",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "clearMocks": true,
    "coverageProvider": "v8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
